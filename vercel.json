{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/static-build",
      "config": {}
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "env": {
    "VITE_WEATHER_API_KEY": "@weather_api_key",
    "VITE_NEWS_API_KEY": "@news_api_key",
    "VITE_ACCUWEATHER_API_KEY": "@accuweather_api_key"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_WEATHER_API_KEY": "@weather_api_key",
    "VITE_NEWS_API_KEY": "@news_api_key",
    "VITE_ACCUWEATHER_API_KEY": "@accuweather_api_key"
  }
}